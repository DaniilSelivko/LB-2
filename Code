#include <iostream>
using namespace std;
int main() {
	cout << "Integer 25" << endl;
	int K, m;
	cout << "K:";
	cin >> K;
	cout << endl;

	m = (K + 3) % 7;
	cout << "number of the day of the week:" << m << endl;
	cout << "Boolean 24" << endl;
	double a2, b2, c2;
	double D;
	cout << "A:";
	cin >> a2;
	cout << "B:";
	cin >> b2;
	cout << "C:";
	cin >> c2;
	D = (b2 * b2) - (4 * a2 * c2);

	if (D >= 0) {
		cout << "True" << endl;
	}
	else {
		cout << "False" << endl;
}
	cout << "Math 5" << endl;
	double x = 0,y=0, num = 0, denum = 0;
	cout << "X:";
	cin >> x;
	cout << endl;

	num = 5 * pow(3, x) * sqrt(exp(x) * abs(sin(27 * (3.14 / 180) + x)));
	denum = (log(2) / log(5)) * abs(x);
	y = num / denum;
	cout << "Result:" <<y<< endl;
	return 0;
}
